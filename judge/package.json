{
  "name": "codemonster-judge",
  "version": "1.0.0",
  "description": "CodeMonster Judge Service - Codeforces-style competitive programming judge",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rm -rf dist",
    "docker:build": "npm run docker:build:python && npm run docker:build:java && npm run docker:build:cpp",
    "docker:build:python": "docker build -t codemonster-python:latest ./docker/python",
    "docker:build:java": "docker build -t codemonster-java:latest ./docker/java",
    "docker:build:cpp": "docker build -t codemonster-cpp:latest ./docker/cpp"
  },
  "keywords": [
    "competitive-programming",
    "online-judge",
    "codeforces-style",
    "code-execution",
    "docker",
    "typescript"
  ],
  "author": "CodeMonster Team",
  "license": "MIT",
  "dependencies": {
    "dockerode": "^4.0.2",
    "uuid": "^10.0.0",
    "fs-extra": "^11.2.0",
    "bullmq": "^5.7.14",
    "ioredis": "^5.4.1",
    "express": "^4.19.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/uuid": "^10.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/dockerode": "^3.3.29",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "typescript": "^5.4.5",
    "ts-node": "^10.9.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.12",
    "ts-jest": "^29.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
